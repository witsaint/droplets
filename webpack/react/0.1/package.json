  "version": "1.0.0",
  "description": "web mall",
  "main": "dist/bundle.js",
  "repository": "",
  "author": "guoxing",
  "private": true,
  "scripts": {
    "story": "start-storybook -p 3335",
    "build-storybook": "build-storybook",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.js --inline --progress",
    "start": "concurrently --kill-others \"npm run dev\" \"npm run story\" ",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "build:stage": "cross-env NODE_ENV=production webpack --config config/webpack.staging.js",
    "lint": "eslint ./src/ --ext .js --ext .jsx --ignore-pattern __tests__",
    "fix": "eslint ./src/ --fix --ext .js --ext .jsx --ignore-pattern __tests__",
    "test": "jest",
    "cover": "jest --coverage",
    "tw": "jest --watch",
    "storybook": "start-storybook -p 6006"
  },
  "optionalDependencies": {
    "@storybook/addon-actions": "^4.0.0",
    "@storybook/addon-links": "^4.0.0",
    "@storybook/addons": "^4.0.0",
    "@storybook/react": "^4.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0",
