    "setupTestFrameworkScriptFile": "<rootDir>/test_setup.js",
    "browser": true,
    "testRegex": "__tests__/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/testfileMock.js",
      "\\.(css|less)$": "<rootDir>/testfileMock.js"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write --trailing-comma es5 --single-quote",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  }
